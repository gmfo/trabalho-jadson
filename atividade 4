#include <stdio.h>

int main(){
    float a, b, c, media;

    printf("digite o primeiro numero:");
    scanf("%f", & a);

    printf("digite o segundo numero:");
    scanf("%f", & b);

    printf("digite o terceiro numero:"):
    scanf("%f", & c);

    media = (a + b + c) / 3;

    printf("A media aritmética dos tres numero é %f", media);
        
        return 0;
    }
